{
  "name": "jetbrains-financo",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "jetbrains",
  "runServices": [
    "redis",
    "postgres"
  ],
  "workspaceFolder": "/workspace",
  "shutdownAction": "stopCompose",
  "postAttachCommand": {
    "go:install:dependencies": "go mod download",
    "npm:install:dependencies": "pnpm install",
    "setup:database": "make db-setup"
  }
}
